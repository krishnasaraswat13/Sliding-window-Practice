class Solution {
    public int numSubarrayProductLessThanK(int[] nums, int k) {
         int i=0;
        int j=0;
      int cnt=0;
        long pro=1;
        while(j<nums.length){
            pro*=nums[j];
           
             while ( i<=j && pro>=k){
                pro/=nums[i];
                i++;
               
            }
           
            cnt+=j-i+1;
            j++;
        }
      
    
        return cnt;
    }
}